  type             String
  is_blank         Boolean            @default(false)
  funnel_prebuilds funnel_prebuilds[]

  @@index([archived_at], map: "acelle_templates_archived_at_index")
}

model activations {
  id           Int       @id @default(autoincrement()) @db.UnsignedInt
