//  Task Management Enums start
enum WorkspaceRole {
  owner
  admin
  member
}

enum TaskManagementStatus {
  TODO
  IN_PROGRESS
  COMPLETED
  CANCELLED
}

enum TaskManagementPriority {
  LOW
  MEDIUM
  HIGH
  URGENT
}

enum RepeatInterval {
  DAILY
  WEEKLY
  MONTHLY
  YEARLY
}

enum ReminderChannel {
  WHATSAPP
  EMAIL
  PUSH_NOTIFICATION
  SMS
}

enum ReminderTriggerType {
  BEFORE
  AFTER
}

enum ReminderStatus {
  PENDING
  SENT
  FAILED
}

enum TaskGroupStatus {
  ACTIVE
  INACTIVE
  ARCHIVED
}

enum TaskGroupType {
  PRIVATE
  PUBLIC
}

enum IdeaStatus {
  DRAFT
  ACTIVE
  UNDER_REVIEW
  APPROVED
  IMPLEMENTED
  REJECTED
