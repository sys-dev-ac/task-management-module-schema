// ==========================================
// TASK MANAGEMENT MODELS
// ==========================================

model TaskManagement {
  id             String                 @id @default(cuid())
  workspace_id   String
  title          String                 @db.VarChar(500)
  description    String?                @db.Text
  status         TaskManagementStatus   @default(TODO)
  priority       TaskManagementPriority @default(MEDIUM)
  dueDate        DateTime?              @db.Timestamp(0)
  completedAt    DateTime?              @db.Timestamp(0)
  category_id    String?
  parent_task_id String?
  group_id       String?
  created_at     DateTime               @default(now()) @db.Timestamp(0)
  updated_at     DateTime               @updatedAt @db.Timestamp(0)
  created_by     BigInt                 @db.UnsignedBigInt
  updated_by     BigInt?                @db.UnsignedBigInt

  workspace   Workspace                @relation(fields: [workspace_id], references: [id], onDelete: Cascade)
  category    TaskCategory?            @relation(fields: [category_id], references: [id], onDelete: SetNull)
  creator     users                     @relation("TaskCreator", fields: [created_by], references: [id])
  updater     users?                    @relation("TaskUpdater", fields: [updated_by], references: [id])
  parentTask  TaskManagement?          @relation("TaskHierarchy", fields: [parent_task_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  subTasks    TaskManagement[]         @relation("TaskHierarchy")
  group       TaskGroup?               @relation(fields: [group_id], references: [id], onDelete: SetNull)
  repeat      TaskRepeat?
  reminders   TaskReminder[]
  checklist   ChecklistItem[]
  comments    TaskComment[]
  history     TaskHistory[]
  assignments TaskAssignment[]
  loops       TaskLoop[]
  attachments TaskAttachment[]

  @@index([workspace_id])
  @@index([category_id])
  @@index([created_by])
  @@index([group_id])
  @@index([parent_task_id])
  @@index([status])
  @@index([priority])
  @@index([dueDate])
  @@map("tm_tasks")
}

model TaskRepeat {
  id              String         @id @default(cuid())
  task_id         String?        @unique
  interval        RepeatInterval
  startDate       DateTime       @db.Date
  endDate         DateTime?      @db.Date
  dayOfMonth      Int?           @db.TinyInt
  skipWeekends    Boolean        @default(false)
  occurrenceCount Int?           @db.UnsignedInt
  created_at      DateTime       @default(now()) @db.Timestamp(0)
  updated_at      DateTime       @updatedAt @db.Timestamp(0)

  task          TaskManagement?    @relation(fields: [task_id], references: [id], onDelete: Cascade)
  taskTemplates TaskTemplate[]
  daysOfWeek    RepeatDayOfWeek[]

  @@index([task_id])
  @@map("tm_task_repeats")
}

model RepeatDayOfWeek {
  id        String @id @default(cuid())
  repeat_id String
  dayOfWeek Int    @db.TinyInt // 0=Sunday, 6=Saturday

  repeat TaskRepeat @relation(fields: [repeat_id], references: [id], onDelete: Cascade)

  @@index([repeat_id])
  @@map("tm_repeat_days_of_week")
}

model TaskReminder {
  id              String               @id @default(cuid())
  task_id         String
  channel         ReminderChannel
  triggerType     ReminderTriggerType
  triggerAt       DateTime             @db.Timestamp(0)
  status          ReminderStatus       @default(PENDING)
  reminderMessage String?              @db.Text
  retryCount      Int                  @default(0) @db.UnsignedTinyInt
  sentAt          DateTime?            @db.Timestamp(0)
  created_at      DateTime             @default(now()) @db.Timestamp(0)
  updated_at      DateTime             @updatedAt @db.Timestamp(0)

  task       TaskManagement      @relation(fields: [task_id], references: [id], onDelete: Cascade)
  recipients ReminderRecipient[]

  @@index([task_id])
  @@index([status])
  @@index([triggerAt])
  @@map("tm_task_reminders")
}

model ReminderRecipient {
