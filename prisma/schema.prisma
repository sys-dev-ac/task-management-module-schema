generator client {
  provider = "prisma-client-js"
}

generator dbml {
  provider = "prisma-dbml-generator"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TestUser {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime
}

model accounts {
  id              BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  user_id         BigInt
  account_type    String?
  account_details String?   @db.Text
  status          Int       @default(1) @db.TinyInt
  is_default      Int       @default(1) @db.TinyInt
  created_at      DateTime? @db.Timestamp(0)
  updated_at      DateTime? @db.Timestamp(0)
}

model acelle_campaigns {
  id                                    Int       @id @default(autoincrement()) @db.UnsignedInt
  uid                                   String    @db.Char(36)
  customer_id                           Int       @db.UnsignedInt
  type                                  String    @db.Text
  name                                  String    @db.Text
  subject                               String    @db.Text
  plain                                 String    @db.LongText
  from_email                            String    @db.Text
  from_name                             String    @db.Text
  reply_to                              String    @db.Text
  status                                String    @db.Text
  sign_dkim                             Boolean
  track_open                            Boolean
  track_click                           Boolean
  resend                                Int
  custom_order                          Int
  run_at                                DateTime? @db.Timestamp(0)
  delivery_at                           DateTime? @db.Timestamp(0)
  created_at                            DateTime? @db.Timestamp(0)
  updated_at                            DateTime? @db.Timestamp(0)
  template_source                       String    @db.Text
  last_error                            String?   @db.Text
  image                                 String
  default_mail_list_id                  Int?      @db.UnsignedInt
  tracking_domain_id                    BigInt?   @db.UnsignedBigInt
  use_default_sending_server_from_email Boolean   @default(false)
  preheader                             String?   @db.Text
  running_pid                           Int?
  template_id                           Int?      @db.UnsignedInt
  skip_failed_message                   Boolean   @default(false)

  @@index([default_mail_list_id], map: "acelle_campaigns_default_mail_list_id_foreign")
}

model acelle_languages {
  id          Int       @id @default(autoincrement()) @db.UnsignedInt
  uid         String    @db.Char(36)
  name        String
  code        String
  region_code String
  status      String
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
  is_default  Boolean   @default(false)
}

model acelle_pages {
  id              Int       @id @default(autoincrement()) @db.UnsignedInt
  uid             String    @db.Char(36)
  layout_id       Int       @db.UnsignedInt
  mail_list_id    Int       @db.UnsignedInt
  content         String    @db.LongText
  created_at      DateTime? @db.Timestamp(0)
  updated_at      DateTime? @db.Timestamp(0)
  subject         String
  use_outside_url Boolean   @default(false)
  outside_url     String?
}

model acelle_settings {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name       String
  value      String?   @db.Text
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

model acelle_subscribers {
  id                       Int       @id @default(autoincrement()) @db.UnsignedInt
  uid                      String    @db.Char(36)
  mail_list_id             Int       @db.UnsignedInt
  email                    String
  status                   String
  from                     String
  ip                       String
  created_at               DateTime? @db.Timestamp(0)
  updated_at               DateTime? @db.Timestamp(0)
  tags                     String?   @db.Text
  verification_status      String?   @db.VarChar(100)
  last_verification_at     DateTime? @db.DateTime(0)
  last_verification_by     String?   @db.VarChar(100)
  last_verification_result String?   @db.MediumText
  import_batch_id          String?   @db.Char(36)

  @@index([email], map: "acelle_subscribers_email_index")
  @@index([mail_list_id, email], map: "acelle_subscribers_mail_list_id_email_index")
}

model acelle_templates {
  id               Int                @id @default(autoincrement()) @db.UnsignedInt
  uid              String             @db.Char(36)
  customer_id      Int?               @db.UnsignedInt
  name             String
  subject          String?
  from_name        String?
  from_email       String?
  reply_to         String?
  content          String             @db.LongText
  custom_order     Int
  created_at       DateTime?          @db.Timestamp(0)
  updated_at       DateTime?          @db.Timestamp(0)
  archived_at      DateTime?          @db.Timestamp(0)
